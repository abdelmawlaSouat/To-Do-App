{
	"env": {
			"browser": true,
			"es2021": true
	},
	"extends": [
			"plugin:react/recommended",
			"airbnb",
			"prettier"
	],
	"parserOptions": {
			"ecmaFeatures": {
					"jsx": true
			},
			"ecmaVersion": 12,
			"sourceType": "module"
	},
	"plugins": [
			"react",
			"material-ui"
	],
	"rules": {
			"react/react-in-jsx-scope": "off",
			"react/jsx-indent": "off",
			"react/jsx-indent-props": "off",
			"react/prop-types": "warn",
			"react/jsx-curly-newline": "off",
			"no-shadow": "off"
	}
}
